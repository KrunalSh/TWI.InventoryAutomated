@model TWI.InventoryAutomated.Models.StockCountIterations
@{
    Layout = null;
}

@using (Html.BeginForm("CreateNewIteration", "StockCount", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.ID)
    //Model.SCID = Convert.ToInt32(ViewBag.SCID);
    <div class="form-group txtNotification">
        @Html.LabelFor(model => model.IterationName, TWI.InventoryAutomated.Resources.GlobalResource.IterationName, new { @class = "LabelAttribs" })
        @Html.EditorFor(model => model.IterationName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group" style="float:right !important;">
        <input type="submit" value="@TWI.InventoryAutomated.Resources.GlobalResource.Save" class="btn btn-primary" />
        <input type="reset" value="@TWI.InventoryAutomated.Resources.GlobalResource.Cancel" class="btn" onclick="Cancelbutton();" />
    </div>
}

